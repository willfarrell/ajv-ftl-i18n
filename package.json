{
  "name": "fluent-transpiler",
  "version": "0.0.0",
  "description": "Transpile Fluent (ftl) files into optomized, tree-shakable, JavaScript EcmaScript Modules (esm).",
  "main": "index.js",
  "type": "module",
  "bin": {
    "ftl": "cli.js"
  },
  "files":[
    "cli.js",
    "index.js",
    "transpile.js",
    "locale/*.js"
  ],
  "scripts": {
    "build":"./bin/build",
    "test": "npm run build && npm run test:unit",
    "test:unit": "node --test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/willfarrell/fluent-transpiler.git"
  },
  "keywords": [
    "Fluent",
    "@fluent",
    "ftl",
    "localization",
    "l10n",
    "internationalization",
    "i18n",
    "ast",
    "compiler",
    "transpiler"
  ],
  "author": "willfarrell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/willfarrell/fluent-transpiler/issues"
  },
  "homepage": "https://middy.js.org",
  "dependencies": {
    "fluent-transpiler": "0.0.1",
    "commander": "9.4.0"
  },
  "devDependencies": {
    "ajv": "8.11.0",
    "ajv-errors": "3.0.0",
    "ajv-formats": "2.1.1",
    "ajv-i18n": "4.2.0",
    "c8": "7.12.0"
  }
}
